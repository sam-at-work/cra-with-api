{
  "name": "server",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "express": "^4.16.3"
  },
  "scripts": {
    "start:dev": "nodemon src/app --exec babel-node",
    "start:prod": "NODE_ENV=production node build/app.js",
    "build": "babel src/ --out-dir build/ --delete-dir-on-start",
    "now-build": "# Skip build step",
    "now-start": "yarn start:prod"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.54",
    "@babel/core": "^7.0.0-beta.54",
    "@babel/node": "^7.0.0-beta.54",
    "@babel/preset-env": "^7.0.0-beta.54",
    "nodemon": "^1.18.2"
  },
  "engines": {
    "node": "v10.6.0"
  }
}
